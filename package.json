{
  "name": "throttleai",
  "version": "0.1.2",
  "description": "Lightweight, token-based AI governance for TypeScript",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./adapters/fetch": {
      "import": {
        "types": "./dist/adapters/fetch.d.ts",
        "default": "./dist/adapters/fetch.js"
      },
      "require": {
        "types": "./dist/adapters/fetch.d.cts",
        "default": "./dist/adapters/fetch.cjs"
      }
    },
    "./adapters/openai": {
      "import": {
        "types": "./dist/adapters/openai.d.ts",
        "default": "./dist/adapters/openai.js"
      },
      "require": {
        "types": "./dist/adapters/openai.d.cts",
        "default": "./dist/adapters/openai.cjs"
      }
    },
    "./adapters/tools": {
      "import": {
        "types": "./dist/adapters/tools.d.ts",
        "default": "./dist/adapters/tools.js"
      },
      "require": {
        "types": "./dist/adapters/tools.d.cts",
        "default": "./dist/adapters/tools.cjs"
      }
    },
    "./adapters/express": {
      "import": {
        "types": "./dist/adapters/express.d.ts",
        "default": "./dist/adapters/express.js"
      },
      "require": {
        "types": "./dist/adapters/express.d.cts",
        "default": "./dist/adapters/express.cjs"
      }
    },
    "./adapters/hono": {
      "import": {
        "types": "./dist/adapters/hono.d.ts",
        "default": "./dist/adapters/hono.js"
      },
      "require": {
        "types": "./dist/adapters/hono.d.cts",
        "default": "./dist/adapters/hono.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src test",
    "typecheck": "tsc --noEmit",
    "check": "pnpm typecheck && pnpm lint && pnpm test && pnpm build && node scripts/bundle-check.js && node scripts/size-guard.js",
    "audit:prod": "node scripts/security-audit.mjs --prod",
    "audit": "node scripts/security-audit.mjs",
    "release:check": "node scripts/require-clean-version.mjs",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ai",
    "throttle",
    "governance",
    "rate-limit",
    "concurrency",
    "lease",
    "llm"
  ],
  "author": "mcp-tool-shop",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcp-tool-shop-org/ThrottleAI.git"
  },
  "packageManager": "pnpm@10.28.2",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "esbuild": ">=0.25.0"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.55.0",
    "vitest": "^2.0.0"
  }
}
